<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.artplanet.myapp.dao.IExhImageRepository">

	<select id="getExhImageList" resultType="com.artplanet.myapp.model.ExhibitionImageVO">
		select *
		from exhibition_file_info
		where exhibition_no = #{exhibition_no}
	</select>
	
</mapper>