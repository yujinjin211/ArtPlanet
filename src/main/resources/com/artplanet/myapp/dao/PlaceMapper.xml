<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.artplanet.myapp.dao.IPlaceInfoRepository">

	<select id="readPlaceInfo" resultType="com.artplanet.myapp.model.Place_InfoVO">
		select *
		from place_info
		where place_no = #{place_no}
	</select>

</mapper>